<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.economy.mapper.IndexModuleMapper">

    <select id="queryAll" resultType="IndexModule">
        SELECT `id` AS id,
                `name` AS name,
                `icon_class` AS iconClass,
                `sort_no` AS sortNo
        FROM `t_index_module`
        ORDER BY `sort_no`
    </select>

</mapper>